  "keywords": [
    "openclaw",
    "quota-tracking",
    "api-monitoring",
    "tos-compliance",
    "sliding-windows",
    "429-prevention",
    "x402",
    "agent-infrastructure",
    "ai",
    "rate-limit-compliance"
  ],
  "author": "AtlasPA",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "express": "^4.18.2",
    "commander": "^12.0.0"
  },
  "openclaw": {
    "skill": true,
    "hooks": {
      "provider:before": "hooks/provider-before.js",
      "provider:after": "hooks/provider-after.js",
      "session:end": "hooks/session-end.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AtlasPA/openclaw-api-quota-tracker.git"
  },
  "homepage": "https://github.com/AtlasPA/openclaw-api-quota-tracker#readme"
